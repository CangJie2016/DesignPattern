<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cangjie.designpatternexercise">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimpleFactoryActivity" />
        <activity android:name=".AbstractFactoryActivity" />
        <activity android:name=".SingletonActivity" />
        <activity android:name=".PrototypeActivity" />
        <activity android:name=".BuilderActivity" />
        <activity android:name=".AdapterActivity" />
        <activity android:name=".CompositeActivity" />
        <activity android:name=".DecoratorActivity" />
        <activity android:name=".FacadeActivity" />
        <activity android:name=".FlyWeightActivity" />
        <activity android:name=".ProxyActivity" />
        <activity android:name=".ChainOfResponsibilityActivity" />
        <activity android:name=".CommandActivity" />
        <activity android:name=".InterpreterActivity" />
        <activity android:name=".IteratorActivity" />
        <activity android:name=".MementoActivity"></activity>
    </application>

</manifest>